{
    "expo": {
        "name": "primo-app",
        "slug": "primo-app",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/logos/logo-primo.png",
        "userInterfaceStyle": "light",
        "newArchEnabled": false,
        "splash": {
            "image": "./assets/images/logos/splash-primo.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["assets/fonts/*"],
        "ios": {
            "supportsTablet": true,
            "splash": {
                "image": "./assets/images/logos/splash-primo.png",
                "resizeMode": "cover",
                "backgroundColor": "#ffffff"
            }
        },
        "android": {
            "adaptiveIcon": {
                "image": "./assets/images/logos/splash-primo.png",
                "backgroundColor": "#ffffff"
            },
            "permissions": ["ACCESS_FINE_LOCATION"]
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
